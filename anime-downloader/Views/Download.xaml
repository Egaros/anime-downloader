<UserControl x:Class="anime_downloader.Views.Download"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:anime_downloader.Views"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:anime_downloader.ViewModels"
             d:DataContext="{d:DesignInstance viewModels:DownloadViewModel}"
             d:DesignHeight="300"
             d:DesignWidth="300"
             Background="BlanchedAlmond"
             mc:Ignorable="d">
    <ContentPresenter Content="{Binding Display}" />
</UserControl>
